{
  "name": "@mtngtools/frame-vue",
  "version": "0.1.0",
  "description": "Vue.js component and composable library for mtngTOOLS",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit",
    "lint": "oxlint src"
  },
  "peerDependencies": {
    "vue": ">=3.5.0"
  },
  "keywords": ["vue", "components", "composables", "mtngtools"],
  "license": "MIT",
  "publishConfig": { "access": "public" },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "oxlint": "^1.26.0",
    "rimraf": "^6.1.2",
    "typescript": "^5.7.0",
    "vite": "8.0.0-beta.13",
    "vite-plugin-dts": "^4.5.4",
    "vue": "^3.5.13"
  }
}
